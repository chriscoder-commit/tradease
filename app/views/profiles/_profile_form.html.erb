
hi <%= current_user.username %>

<%= form_with model: @profile, local: true do |form| %>
 <%= form.label :name %>
  <%= form.text_field :name %>
  
  <%= form.label :description %>
  <%= form.text_field :description %>
  
  <%= form.label :place_of_business %>
  <%= form.text_field :place_of_business %>

  <%= form.label :about_me %>
  <%= form.text_field :about_me %>

  <%= form.label :Skills %>
  <%= form.select :skills, options_for_select(Profile.skills.keys) %>

  <%= form.submit id: "submit" %>
<% end %>





